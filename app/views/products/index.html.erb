<h1>Coffee Supplies</h1>

<% @products.each do |product| %>
  <p> Name: <%= product.name %> </p>
  <p> Price: <%= product.price %> </p>
  <p> Description: <%= product.description %> </p>
  <p> Instock: <%= product.instock? %> </p>
  <p> <a href="/products/<% product.id %>">More info</a>
  <hr>
<% end %>
  